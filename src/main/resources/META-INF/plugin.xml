<idea-plugin>
    <id>edu.team449.YamlPlugin</id>
    <name>YamlPlugin</name>
    <vendor>The Blair Robot Project</vendor>

    <description>Checks YAML files for robot maps,
        hopefully adds safety</description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.java</depends>
    <depends>com.intellij.modules.java</depends>
    <depends>org.jetbrains.plugins.yaml</depends>

    <extensions defaultExtensionNs="com.intellij">

        <psi.referenceContributor implementation="edu.team449.lang.MyYamlReferenceContributor"
                                  language=""/>
        <lang.findUsagesProvider language="JAVA" implementationClass="edu.team449.lang.MyJavaFindUsagesProvider"/>
        <annotator implementationClass="edu.team449.lang.MyYamlAnnotator" language=""/>
        <!--externalAnnotator implementationClass="edu.team449.lang.YamlExternalAnnotator"
                language=""/-->
        <gotoSymbolContributor implementation="edu.team449.lang.YamlGoToSymbolContributor"/>
        <completion.contributor implementationClass="edu.team449.lang.YamlCompletionContributor"
                                language="any"/>

    </extensions>

    <actions>

    </actions>
</idea-plugin>